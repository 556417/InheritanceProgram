// why did i add this many ducks god help me
public class BlueDuck extends Ducks{
    private int waterTemperature;
    private int waterAmount;

    public BlueDuck(double size, int age, String ableToFly, int waterTemperature, int waterAmount){
        super(size, age, "blue", ableToFly);
        this.waterTemperature = waterTemperature;
        this.waterAmount = waterAmount;
    }

    public int getWaterTemperature(){
        return(this.waterTemperature);
    }
    public void setWaterTemperature(int waterTemperature(){
        this.waterTemperature = waterTemperature; 
    }
    public int getWaterAmount(){
        return(this.waterAmount);
    }
    public void setWaterAmount(int waterAmount){
        this.waterAmount = waterAmount; 
    }

    public String toString{
        return("The blue duck is " + this.getSize() + "kg and is " + this.getAge() + " years old. It " + this.getFly() + " fly." + 
                "It sprays " + this.getWaterTemperature() + " degrees of water with a total volume of " + this.getWaterAmount() + " gallons.");
    }

}
